<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/file_path"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="filepath"
        android:textStyle="bold"
        android:layout_marginTop="6dp"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:singleLine="true"
        android:fontFamily="sans-serif-black"
        android:textSize="14dp"
        android:textAlignment="viewStart"
        />

    <TextView
        android:id="@+id/file_path_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="file path value"
        android:textColor="@color/design_default_color_primary_dark"
        android:textStyle="normal"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/file_path"
        />

    <TextView
        android:id="@+id/sms_from_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sms from date"
        android:textStyle="bold"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/file_path_value"
        />

    <TextView
        android:id="@+id/sms_from_date_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="isms from date value"
        android:textColor="@color/design_default_color_primary_dark"
        android:textStyle="normal"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/sms_from_date"
        />


    <TextView
        android:id="@+id/sms_to_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sms to date"
        android:textStyle="bold"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/file_path_value"
        />

    <TextView
        android:id="@+id/sms_to_date_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sms to date value"
        android:textColor="@color/design_default_color_primary_dark"
        android:textStyle="normal"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/sms_to_date"
        />

    <TextView
        android:id="@+id/number_of_sms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of sms"
        android:textStyle="bold"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/sms_to_date_value"
        />

    <TextView
        android:id="@+id/number_of_sms_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of sms value"
        android:textColor="@color/design_default_color_primary_dark"
        android:textStyle="normal"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/number_of_sms"
        />

    <TextView
        android:id="@+id/number_of_mobile_nr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of mobile numbers"
        android:textStyle="bold"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/number_of_sms_value"
        />

    <TextView
        android:id="@+id/number_of_mobile_nr_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of mobile numbers"
        android:textColor="@color/design_default_color_primary_dark"
        android:textStyle="normal"
        android:layout_marginRight="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="2dp"
        android:singleLine="false"
        android:fontFamily="sans-serif-condensed"
        android:textSize="12dp"
        android:textAlignment="viewStart"
        android:layout_below="@+id/number_of_sms_value"
        />

</LinearLayout>